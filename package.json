{
	"name": "reboot-client-ssr",
	"description": "sveltekit client yugioh",
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": ">=12.17.0"
	},
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@mdi/js": "^5.9.55",
		"@playwright/test": "^1.21.1",
		"@sveltejs/adapter-node": "^1.0.0-next.45",
		"@sveltejs/kit": "^1.0.0-next.162",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^4.30.0",
		"@typescript-eslint/parser": "^4.30.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.4.0",
		"sass": "^1.39.0",
		"svelte": "^3.42.4",
		"svelte-check": "^2.2.5",
		"svelte-preprocess": "^4.8.0",
		"tslib": "^2.3.1",
		"typescript": "^4.4.2"
	},
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@lukeed/uuid": "^2.0.0",
		"cookie": "^0.4.1",
		"svelte-materialify": "^0.3.9"
	},
	"author": {
		"name": "reboot-lyon",
		"url": "https://rebootlyon.fr"
	},
	"contributors": [
		"lloyet"
	],
	"repository": {
		"url": "https://github.com/reboot-lyon/client-ssr"
	},
	"bugs": {
		"url": "https://github.com/reboot-lyon/client-ssr"
	}
}
